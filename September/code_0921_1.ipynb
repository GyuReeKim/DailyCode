   "source": [
    "# 정삼각형 분할 놀이\n",
    "\n",
    "T = int(input())\n",
    "for tc in range(1, T+1):\n",
    "    A, B = list(map(int, input().split()))\n",
    "    N = A//B\n",
    "    print(N)\n",
    "    result = 0\n",
    "    for i in range(N):\n",
    "        result += (2*i+1)\n",
    "    print('#{} {}'.format(tc, result))"
   ]
  }
